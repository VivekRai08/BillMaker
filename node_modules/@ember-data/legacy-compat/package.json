{
  "name": "@ember-data/legacy-compat",
  "description": "Compatibility Shims for Older EmberData",
  "version": "5.6.0",
  "license": "MIT",
  "author": "Chris Thoburn <runspired@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:emberjs/data.git",
    "directory": "packages/legacy-compat"
  },
  "homepage": "https://github.com/emberjs/data",
  "bugs": "https://github.com/emberjs/data/issues",
  "keywords": [
    "ember-addon"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "files": [
    "unstable-preview-types",
    "addon-main.cjs",
    "dist",
    "README.md",
    "LICENSE.md",
    "logos"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./*": {
      "default": "./dist/*.js"
    },
    "./unstable-preview-types": {
      "types": "./unstable-preview-types/index.d.ts"
    }
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "dependencies": {
    "@embroider/macros": "^1.16.12",
    "@warp-drive/core": "5.6.0",
    "@warp-drive/legacy": "5.6.0",
    "@warp-drive/utilities": "5.6.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-typescript": "^7.27.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@warp-drive/internal-config": "5.6.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  },
  "ember": {
    "edition": "octane"
  },
  "typesVersions": {
    "*": {
      "unstable-preview-types": [
        "./unstable-preview-types"
      ]
    }
  },
  "scripts": {
    "lint": "eslint . --quiet --cache --cache-strategy=content",
    "build:pkg": "vite build;",
    "sync": "echo \"syncing\"",
    "start": "vite"
  }
}