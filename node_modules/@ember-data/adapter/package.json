{
  "name": "@ember-data/adapter",
  "version": "5.6.0",
  "description": "Provides Legacy JSON:API and REST Implementations of the Adapter Interface for use with @ember-data/store",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:emberjs/data.git",
    "directory": "packages/adapter"
  },
  "license": "MIT",
  "author": "",
  "directories": {},
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "files": [
    "unstable-preview-types",
    "blueprints",
    "addon-main.cjs",
    "dist",
    "README.md",
    "LICENSE.md",
    "logos"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./blueprints/*": {
      "default": "./blueprints/*.js"
    },
    "./*": {
      "default": "./dist/*.js"
    },
    "./unstable-preview-types": {
      "types": "./unstable-preview-types/index.d.ts"
    }
  },
  "peerDependencies": {},
  "dependencies": {
    "@embroider/macros": "^1.16.12",
    "ember-cli-test-info": "^1.0.0",
    "ember-cli-string-utils": "^1.1.0",
    "ember-cli-path-utils": "^1.0.0",
    "@ember/edition-utils": "1.2.0",
    "@warp-drive/core": "5.6.0",
    "@warp-drive/legacy": "5.6.0",
    "@warp-drive/utilities": "5.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-typescript": "^7.27.0",
    "@babel/preset-typescript": "^7.27.0",
    "@warp-drive/internal-config": "5.6.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "typesVersions": {
    "*": {
      "unstable-preview-types": [
        "./unstable-preview-types"
      ]
    }
  },
  "scripts": {
    "lint": "eslint . --quiet --cache --cache-strategy=content",
    "build:pkg": "vite build;",
    "sync": "echo \"syncing\"",
    "start": "vite"
  }
}